{
  "name": "app-client",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "build:js": "webpack",
    "watch:js": "onchange 'src/**/*.js' -- npm run build:js",
    "cp:html": "copyfiles -u 1 src/index.html public",
    "watch:html": "onchange 'src/*.html' -- npm run cp:html",
    "watch:css": "onchange 'src/**/*.css' -- npm run build:js",
    "build": "npm-run-all --parallel build:js cp:html",
    "watch": "npm-run-all --parallel watch:js watch:html watch:css"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "4.0.0-alpha.6",
    "font-awesome": "^4.7.0",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^1.2.0",
    "reactstrap": "^4.8.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "copyfiles": "^1.2.0",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.5",
    "npm-run-all": "^4.1.2",
    "onchange": "^3.2.1",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1"
  }
}
