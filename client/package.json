{
  "name": "app-client",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "build:js": "webpack",
    "watch:js": "onchange 'src/**/*.js' -- npm run build:js",
    "cp:html": "copyfiles -u 1 src/index.html public",
    "watch:html": "onchange 'src/*.html' -- npm run cp:html",
    "watch:css": "onchange 'src/**/*.css' -- npm run build:js",
    "build": "npm-run-all --parallel build:js cp:html",
    "watch": "npm-run-all --parallel watch:js watch:html watch:css"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^4.0.0-alpha.6",
    "font-awesome": "^4.7.0",
    "isomorphic-fetch": "^2.2.1",
    "prop-types": "^15.5.6",
    "react": "^15.5.3",
    "react-addons-css-transition-group": "^15.5.2",
    "react-addons-transition-group": "^15.5.2",
    "react-dom": "^15.5.3",
    "react-redux": "^5.0.4",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "reactstrap": "^4.5.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "copyfiles": "^1.2.0",
    "css-loader": "^0.28.0",
    "file-loader": "^0.11.1",
    "npm-run-all": "^4.0.2",
    "onchange": "^3.2.1",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.2"
  }
}
